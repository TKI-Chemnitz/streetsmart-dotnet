<Window x:Class="StreetSmart.WPF.Example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StreetSmart.WPF.Example"
        xmlns:streetSmart="clr-namespace:StreetSmart.Wpf;assembly=StreetSmart.Wpf"
        xmlns:example="clr-namespace:StreetSmart.WPF.Example;assembly=StreetSmart.WPF.Example"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance example:MainWindowModel}"
        Title="MainWindow" Height="450" Width="800">
  <Grid>
    <streetSmart:StreetSmartGUI
      StreetSmartLocation="{Binding StreetSmartLocation}"
      <!--Api="{Binding Api, Mode=OneWayToSource}" -->
    />
  </Grid>
</Window>
